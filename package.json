{
  "name": "serverless-nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "jest src/__tests__/.*/*.test.tsx?$ && yarn puppeteer-storyshots",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "puppeteer-storyshots": "jest src/__tests__/puppeteer-storyshots*.runner.ts"
  },
  "dependencies": {
    "@testing-library/react": "^11.1.2",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-dom": "^4.0.0",
    "next": "10.0.1",
    "prettier": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "ts-jest": "^26.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-backgrounds": "^6.0.28",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addon-storyshots": "^6.0.28",
    "@storybook/addon-storyshots-puppeteer": "^6.0.28",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/addon-viewport": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "babel-loader": "^8.2.1",
    "husky": "^4.3.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^5.5.0",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.1",
    "styled-media-query": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  }
}
